#include <iostream>
using namespace std;
int main() {   
 int a, b;    a = 11;    b = 19;    bool result; 
   result = (a == b);   // false  
  cout << "11 == 19 is " << result << endl;  
  result = (a != b);  // true   
 cout << "11 != 19 is " << result << endl;   
 result = a > b;   // false   
 cout << "11> 19 is " << result << endl; 
   result = a < b;   // true  
  cout << "11 < 19 is " << result << endl;   
 result = a >= b;  // false   
 cout << "11 >= 19 is " << result << endl;  
  result = a <= b;  // true   
 cout << "11 <= 19 is " << result << endl;  
  return 0;}
